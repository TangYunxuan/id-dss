{
  "name": "id-dss-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:ghp": "vue-tsc && vite build && node scripts/write-cname.mjs",
    "build:path-id-dss": "vue-tsc && VITE_PUBLIC_BASE_PATH=/id-dss/ vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "deploy": "gh-pages -d dist -b gh-pages",
    "deploy:pages-repo": "gh-pages -d dist -b gh-pages -r https://github.com/TangYunxuan/vue-github-pages.git"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "pinia": "^2.1.7",
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vue-tsc": "^2.0.7",
    "@types/node": "^20.11.30",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.38",
    "autoprefixer": "^10.4.19",
    "gh-pages": "^6.3.0"
  }
}

